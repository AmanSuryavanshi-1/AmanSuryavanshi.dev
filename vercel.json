{
  "redirects": [
    {
      "source": "/:path*",
      "has": [
        {
          "type": "host",
          "value": "amansuryavanshi-dev.vercel.app"
        }
      ],
      "destination": "https://amansuryavanshi.me/:path*",
      "permanent": true
    },
    {
      "source": "/:path*",
      "has": [
        {
          "type": "host",
          "value": "www.amansuryavanshi.me"
        }
      ],
      "destination": "https://amansuryavanshi.me/:path*",
      "permanent": true
    }
  ]
}